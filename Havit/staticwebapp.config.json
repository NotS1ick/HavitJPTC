{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/Account/*",
      "methods": ["GET", "POST"],
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/Identity/*",
      "methods": ["GET", "POST"],
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/*",
      "methods": ["GET"],
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/js/*",
      "/api/*"
    ]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "responseOverrides": {
    "401": {
      "redirect": "/Account/Login",
      "statusCode": 302
    }
  }
}
